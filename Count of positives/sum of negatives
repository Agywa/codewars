function countPositivesSumNegatives(input) {
 var a = []
    let positiveSum = 0
    let negativeSum = 0

    if(input && input.length) {
      for(let i = 0; i < input.length; i++) {
        if(input[i] > 0) {
            positiveSum += 1
        } else {
            negativeSum += input[i]
        }
      }
      a.push(positiveSum)
      a.push(negativeSum)
    }
    return a
}
